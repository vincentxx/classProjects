<expression> ::= <term>
              |  <term> '+' <expression>
              |  <term> '-' <expression>
<term> ::= <factor>
        |  <factor> '*' <term>
        |  <factor> '/' <term>
<factor> ::= <number>
          |  '-' <number>
          |  '(' <expression> ')'
          |  '-' '(' <expression> ')'
